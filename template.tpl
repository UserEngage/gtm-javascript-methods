      methodType: 'pageHit',
      apiKey: 'hdPn34'
    };

    runCode(validDataMock);

    assertApi('makeTableMap').wasNotCalled();
    assertApi('callInWindow').wasCalled();
    assertApi('gtmOnSuccess').wasCalled();
