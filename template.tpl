// Setup Google APIs
const log = require('logToConsole');
const queryPermission = require('queryPermission');
const callInWindow = require('callInWindow');
const makeTableMap = require('makeTableMap');
const Object = require('Object');

const SUCCESS_MESSAGE = 'User.com UE |Success| Method Invoked';
const FAILURE_MESSAGE = 'User.com UE |Error| Verify inputs and required permissions';
const FAILURE_MESSAGE_KEY = 'User.com UE |Error| Verify Api Key';
const FAILURE_MESSAGE_METHOD = 'User.com UE |Error| Verify Method Type';
const EMPTY_OBJECT = {};
const EMPTY_ARRAY = [];
const METHOD_MAP = {
  pageHit: 'UE.pageHit',
  destroy: 'UE.destroy',
  resetAuth: 'UE.resetAuth'
};

// Handle User preferences
const logger = data.logEnabled ? log : () => EMPTY_OBJECT;
const arePermissionsGranted = (name) => queryPermission('access_globals', 'execute', name);

const getApiKey = (value) => ({ apiKey: value });
const getMethodBody = (data) => makeTableMap(data, 'name', 'value');


// Get config values
const methodType = data.methodType ? METHOD_MAP[data.methodType] : onFailure(FAILURE_MESSAGE_METHOD);
const attributes = data.attributesTable ? getMethodBody(data.attributesTable) : EMPTY_ARRAY;
const apiKey = data.apiKey ? getApiKey(data.apiKey) : onFailure(FAILURE_MESSAGE_KEY);
const methodBody = mergeObjects(attributes, apiKey);

function onSuccess (message) {
  logger(message);
  data.gtmOnSuccess();
}

function onFailure (message) {
  logger(message);
  data.gtmOnFailure();
}

function mergeObjects () {
  const resObj = EMPTY_OBJECT;
  for (const arg of arguments) {
    const keys = Object.keys(arg);
    for (const key of keys) {
      resObj[key] = arg[key];
      }
    }
  return resObj;
}

function invokeMethod (method, data) {
  callInWindow(method, data);
}


// Execute
if (arePermissionsGranted(methodType)) {
  invokeMethod(methodType, methodBody);
  onSuccess(SUCCESS_MESSAGE);
} else {
  onFailure(FAILURE_MESSAGE);
}


